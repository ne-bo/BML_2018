{
  "name": "Mnist_AAEP",
  "n_gpu": 1,
  "arch": {
    "type": "MnistModel",
    "args": {}
  },
  "data_loader": {
    "type": "MnistDataLoader",
    "args": {
      "data_dir": "data/",
      "batch_size": 64,
      "shuffle": true,
      "validation_split": 0.1,
      "num_workers": 2
    }
  },
  "optimizer_encoder": {
    "type": "RMSprop",
    "args": {
      "lr": 0.0003,
      "weight_decay": 0
      //"amsgrad": true
    }
  },
  "optimizer_decoder": {
    "type": "RMSprop",
    "args": {
      "lr": 0.0003,
      "weight_decay": 0
      //"amsgrad": true
    }
  },
  "optimizer_code_generator": {
    "type": "RMSprop",
    "args": {
      "lr": 0.0003,
      "weight_decay": 0
      //"amsgrad": true
    }
  },
  "optimizer_d_i": {
    "type": "RMSprop",
    "args": {
      "lr": 0.0003,
      "weight_decay": 0
      //"amsgrad": true
    }
  },
  "optimizer_d_c": {
    "type": "RMSprop",
    "args": {
      "lr": 0.00003,
      "weight_decay": 0
      //"amsgrad": true
    }
  },
  "loss": "nll_loss",
  "metrics": [
    "my_metric",
    "my_metric2"
  ],
  "lr_scheduler_freq": 5,
  "lr_scheduler": {
    "type": "ExponentialLR",
    "args": {
      "gamma": 0.9
    }
  },
  "trainer": {
    "epochs": 1000,
    "save_dir": "saved/",
    "save_freq": 1,
    "verbosity": 2,
    "monitor": "loss",
    "monitor_mode": "min"
  },
  "visualization": {
    "tensorboardX": true,
    "log_dir": "saved/runs"
  },
  "generated_images_path": "/home/natasha/generated_images",
  "z_scale": 5.0
}
